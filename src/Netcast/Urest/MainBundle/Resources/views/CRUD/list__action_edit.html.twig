{% if admin.isGranted('EDIT', object) and admin.hasRoute('edit') and (object.deleted is defined and not object.deleted) or object.deleted is not defined %}
    <a href="{{ admin.generateObjectUrl('edit', object) }}" class="btn btn-default btn-sm btn-icon icon-left" title="{{ 'action_edit'|trans({}, 'SonataAdminBundle') }}">
        <i class="entypo-pencil"></i>
        {{ 'action_edit'|trans({}, 'SonataAdminBundle') }}
    </a>
{% endif %}
